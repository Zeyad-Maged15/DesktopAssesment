<Page x:Class="ZeyadMaged_DesktopAssesment.Decide"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ZeyadMaged_DesktopAssesment"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Decide">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/6128.jpg_wh1200.jpg"/>
        </Grid.Background>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="navtosear" Grid.Column="2" Content="Delete Page" HorizontalAlignment="Center" Grid.Row="1" VerticalAlignment="Top" Height="64" Width="160" Margin="0,94,0,0" Grid.RowSpan="2" Click="navtosear_Click"/>
        <Button x:Name="navtodel" Content="Search Page" HorizontalAlignment="Left" VerticalAlignment="Top" Height="64" Width="169" Margin="131,94,0,0" Grid.ColumnSpan="2" Grid.RowSpan="2" Grid.Row="1" Click="navtodel_Click"/>
    </Grid>
</Page>
